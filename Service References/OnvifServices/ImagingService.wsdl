        </xs:complexType>
      </xs:element>
      <xs:element name="Move">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="VideoSourceToken" type="tt:ReferenceToken">
              <xs:annotation>
                <xs:documentation>
							Reference to the VideoSource for the requested move (focus) operation.
							</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Focus" type="tt:FocusMove">
              <xs:annotation>
