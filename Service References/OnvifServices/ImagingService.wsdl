        <xs:complexType>
          <xs:sequence>
            <xs:element name="Status" type="tt:ImagingStatus20">
              <xs:annotation>
                <xs:documentation>
						Requested imaging status.
						</xs:documentation>
              </xs:annotation>
            </xs:element>
