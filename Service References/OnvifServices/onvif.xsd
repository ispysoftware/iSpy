    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="OSDPosConfiguration">
    <xs:sequence>
      <xs:element name="Type" type="xs:string">
        <xs:annotation>
          <xs:documentation>For OSD position type, following are the pre-defined: <ul><li>UpperLeft</li><li>UpperRight</li><li>LowerLeft</li><li>LowerRight</li><li>Custom</li></ul></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Pos" type="tt:Vector" />
