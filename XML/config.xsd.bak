        <xs:element name="MJPEGURL" type="xs:string" />
        <xs:element name="AVIFileName" type="xs:string" />
        <xs:element name="RecentJPGList" type="xs:string" />
        <xs:element name="RecentMJPGList" type="xs:string" />
